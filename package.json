    "license": "ISC",
    "bugs": {
        "url": "https://github.com/unfoldui/web/issues"
    },
    "homepage": "https://github.com/unfoldui/web#readme",
    "dependencies": {
        "concurrently": "^3.5.1",
        "pug": "^2.0.3",
        "pug-cli": "^1.0.0-alpha6"
    },
    "devDependencies": {
        "node-sass": "^4.9.0"
    }
}
